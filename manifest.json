{
    "manifest_version": 3,
    "name": "IMHentai Downloader",
    "version": "1.0",
    "description": "Fast imhentai downloader.",
    "permissions": [
        "activeTab",
        "downloads"
    ],
    "host_permissions": [
      "https://imhentai.xxx/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://imhentai.xxx/*"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ],
    "action": {
      "default_title": "Media Downloader"
    }
  }
  